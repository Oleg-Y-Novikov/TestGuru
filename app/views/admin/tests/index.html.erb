<% provide(:title, t('.all_tests')) %>

<h1><%= t('.all_tests') %></h1>
<div class='button_inline'>
  <%= button_to t('.create_test'), new_admin_test_path, method: :get, class: "btn btn-primary" %>
</div>
<div class='button_inline'>
  <%= button_to t('.show_all_gists'), admin_gists_path, method: :get, class: "btn btn-primary" %>
</div>
<br>
<table class="table">
  <thead>
    <tr>
      <th><%= t('.test') %></th>
      <th><%= t('.level') %></th>
      <th><%= t('.category') %></th>
      <th><%= t('.amount_questions') %></th>
      <th colspan="3"><%= t('global.actions') %></th>
    </tr>
  </thead>
  <%= render @tests %>
</table>
