<% provide(:title, t('.all_gists')) %>

<h1><%= t('.all_gists') %></h1>
<%= button_to t('admin.tests.show_all_tests'), admin_tests_path, method: :get, class: 'btn btn-primary' %>
<br>
<table class="table">
  <thead>
    <tr>
      <th><%= t('.question') %></th>
      <th><%= t('.gist_url') %></th>
      <th><%= t('.user') %></th>
    </tr>
  </thead>
  <%= render @gists %>
</table>
