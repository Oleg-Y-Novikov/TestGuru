<% provide(:title, t('page_questions.all_questions')) %>

<h1><%= @test.title %></h1>
<%= button_to t('page_questions.create_question'), new_test_question_path(@test), method: :get %>

<h3><%= t('page_questions.all_questions') %></h3>

<table>
  <tr>
    <th><%= t('page_questions.question') %></th>
    <th colspan="3"><%= t('global.actions') %></th>
  </tr>
  <%= render @questions %>
</table>
