<% provide(:title, t('page_tests.all_tests')) %>

<h1><%= t('page_tests.all_tests') %></h1>
<%= button_to t('page_tests.create_test'), new_test_path, method: :get, class: "btn btn-primary" %>
<br>
<table class="table">
  <thead>
    <tr>
      <th><%= t('page_tests.test') %></th>
      <th><%= t('page_tests.level') %></th>
      <th><%= t('page_tests.category') %></th>
      <th><%= t('page_tests.amount_questions') %></th>
      <th colspan="4"><%= t('global.actions') %></th>
    </tr>
  </thead>
  <%= render @tests %>
</table>
